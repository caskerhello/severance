<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.third.severance.dao.IDoctorDao">


    <select id="selectSection" resultType="com.third.severance.dto.DoctorVO">
        select * from DOCTOR where doctorsection=#{param1}
    </select>

    <select id="doctorDetail" resultType="com.third.severance.dto.DoctorVO">
        select * from DOCTOR where dseq=#{param1}
    </select>





</mapper>